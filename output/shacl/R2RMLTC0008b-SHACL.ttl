@base <http://cse.ssu.ac.kr/> .
@prefix cse: <http://cse.ssu.ac.kr/#> .
@prefix ex: <http://example.com/> .
@prefix activity: <http://example.com/activity/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

cse:TriplesMap2Shape-rdf-type
    a sh:PropertyShape ;
    sh:path rdf:type ;
    sh:nodeKind sh:IRI ;
    sh:in ( activity:Sport ) ;
    sh:minCount 1 ;
    sh:maxCount 1  .
    
cse:TriplesMap2Shape
    a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "^http://example.com/(.+)$" ;
    sh:property cse:TriplesMap2Shape-rdf-type  .
    
cse:TriplesMap1Shape-ex-Sport
    a sh:PropertyShape ;
    sh:path ex:Sport ;
    sh:node cse:TriplesMap2Shape  .
    
cse:TriplesMap1Shape-ex-id
    a sh:PropertyShape ;
    sh:path ex:id ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:integer ;
    sh:minCount 1 ;
    sh:maxCount 1  .
    
cse:TriplesMap1Shape-foaf-name
    a sh:PropertyShape ;
    sh:path foaf:name ;
    sh:nodeKind sh:Literal ;
    sh:datatype xsd:string ;
    sh:minCount 1 ;
    sh:maxCount 1  .
    
cse:TriplesMap1Shape-rdf-type
    a sh:PropertyShape ;
    sh:path rdf:type ;
    sh:nodeKind sh:IRI ;
    sh:in ( foaf:Person ) ;
    sh:minCount 1 ;
    sh:maxCount 1  .
    
cse:TriplesMap1Shape
    a sh:NodeShape ;
    sh:nodeKind sh:IRI ;
    sh:pattern "^http://example.com/Student/(.+)/(.+)$" ;
    sh:property cse:TriplesMap1Shape-ex-Sport ;
    sh:property cse:TriplesMap1Shape-ex-id ;
    sh:property cse:TriplesMap1Shape-foaf-name ;
    sh:property cse:TriplesMap1Shape-rdf-type  .
    
