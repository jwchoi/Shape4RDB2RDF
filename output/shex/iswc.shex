BASE <http://foo.example/iswc/>
PREFIX foo: <http://foo.example/iswc/#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

foo:conferencesConfID xsd:integer MININCLUSIVE -2147483648 MAXINCLUSIVE 2147483647
foo:conferencesName xsd:string MAXLENGTH 100
foo:conferencesURI xsd:string MAXLENGTH 200
foo:conferencesDate xsd:string MAXLENGTH 50
foo:conferencesLocation xsd:string MAXLENGTH 50
foo:conferencesDatum xsd:dateTime /(^(19[7-9][0-9]|20([0-2][0-9]|3[0-7]))-(0[1-9]|1[0-2])-(0[1-9]|[12][0-9]|3[01])T(0[0-9]|1[0-9]|2[0-3]):([0-5][0-9]):([0-5][0-9])((.[0-9]{1,6}){0})$)|(^2038-01-(0[1-9]|1[0-8])T(0[0-9]|1[0-9]|2[0-3]):([0-5][0-9]):([0-5][0-9])((.[0-9]{1,6}){0})$)|(^2038-01-19T0[0-4]:([0-5][0-9]):([0-5][0-9])((.[0-9]{1,6}){0})$)|(^2038-01-19T05:(0[0-9]|1[0-3]):([0-5][0-9])((.[0-9]{1,6}){0}))|(2038-01-19T05:14:0[0-6]((.[0-9]{1,6}){0})$)|(^2038-01-19T05:14:07((.0{1,6}){0})$)/

foo:conferencesShape IRI
 /^http:\/\/foo\.example\/iswc\/conferences\/ConfID=(.*)$/ {
  a [<conferences>] ;
  <conferences#ConfID> @foo:conferencesConfID ;
  <conferences#Name> @foo:conferencesName? ;
  <conferences#URI> @foo:conferencesURI? ;
  <conferences#Date> @foo:conferencesDate? ;
  <conferences#Location> @foo:conferencesLocation? ;
  <conferences#Datum> @foo:conferencesDatum? ;
 ^<papers#ref-Conference> @foo:papersShape* 
}

