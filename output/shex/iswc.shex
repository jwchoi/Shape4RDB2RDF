  <organizations#URI> @foo:organizationsURI? ;
  <organizations#Belongsto> @foo:organizationsBelongsto? ;
  <organizations#Homepage> @foo:organizationsHomepage? ;
  <organizations#ref-Belongsto> @foo:organizationsShape? ;
 ^<organizations#ref-Belongsto> @foo:organizationsShape* ;
 ^<rel_person_organization#ref-OrganizationID> @foo:rel_person_organizationShape* 
}

