  <papers#Year> @foo:papersYear? ;
  <papers#Conference> @foo:papersConference? ;
  <papers#Publish> @foo:papersPublish? ;
  <papers#ref-Conference> @foo:conferencesShape? ;
 ^<rel_paper_topic#ref-PaperID> @foo:rel_paper_topicShape* ;
 ^<rel_person_paper#ref-PaperID> @foo:rel_person_paperShape* 
}

