foo:organizationsType xsd:string MAXLENGTH 50
foo:organizationsName xsd:string MAXLENGTH 200
foo:organizationsAddress xsd:string MAXLENGTH 65535
foo:organizationsLocation xsd:string MAXLENGTH 50
foo:organizationsPostcode xsd:string MAXLENGTH 10
foo:organizationsCountry xsd:string MAXLENGTH 50
foo:organizationsURI xsd:string MAXLENGTH 100
foo:organizationsBelongsto xsd:integer MININCLUSIVE -2147483648 MAXINCLUSIVE 2147483647
foo:organizationsHomepage xsd:string MAXLENGTH 200

